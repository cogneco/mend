{
	"transform": {
		"^.+\\.tsx?$": "ts-jest"
	},
	"testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
	"testPathIgnorePatterns" : ["node_modules/", "build/" ],
	"collectCoverageFrom": [
		"**/*.{ts,tsx,js,jsx}",
		"!**/node_modules/**",
		"!**/build/**"
	],
	"moduleFileExtensions": [
		"ts",
		"tsx",
		"js",
		"jsx",
		"json",
		"node"
	]
}
